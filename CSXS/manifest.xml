<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="top.gyrojeff.yuzumarker.photoshop" ExtensionBundleVersion="1.0.0" Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
